                    <div class="section-heading blog-margin home-6">
                        <h3>Featured Products</h3>
                    </div>
                </div>
            </div>
            <!--featured-product-area-heading-end-->
            <div class="row">
                <div class="featured-product-carousel single-indicator">
                    <!-- single-product-start -->
                        {% for newProduct in newProducts %}
                            <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
                                <div class="single-product">
                                    <div class="product-img">
                                        <a href="{{ path(newProduct) }}" class="pull-left"
                                           title="{{ 'sylius.ui.view_product_details'|trans({'%product%': newProduct.name})|escape('html_attr') }}">
                                            <img src="{{ newProduct.image ? newProduct.image.path|imagine_filter('sylius_large') : 'http://placehold.it/50x40' }}" alt="" class="primary-img" />
                                        </a>
                                        <div class="product-action">
                                            <div class="pro-rating">
                                                <ul>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                    <li><i class="fa fa-star"></i></li>
                                                </ul>
                                            </div>
                                            <div class="action-link">
                                                <a href="#"  data-toggle="tooltip" title="Wishlist"><i class="fa fa-heart-o"></i></a>
                                                <a href="#"  data-toggle="tooltip" title="Compare"><i class="fa fa-random"></i></a>
                                                <a data-toggle="modal" data-target="#productModal{{ newProduct.id }}" href="#" title="Quick view"><i class="fa fa-eye"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="product-content">
                                        <h2><a href="{{ path(newProduct) }}" >{{ newProduct.name }}</a></h2>
                                        <div class="pro-price">
                                            <p class="normal-price"> {{ newProduct.masterVariant.price|sylius_price }}</p>
                                        </div>
                                        <div class="product-cart">
                                            <button class="button">Add to cart</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        {% endfor %}
                    <!-- single-product-end -->
                </div>
            </div>
