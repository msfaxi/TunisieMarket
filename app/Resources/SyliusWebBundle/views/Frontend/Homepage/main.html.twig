    <div class="banner-area home-6">
        <div class="container">
            <div class="row">
                <!-- single-banner start -->
                    {% for taxon in taxons %}
                        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                            <div class="single-banner home-3 home-6">
                                <a href="{{ url(taxon.getPermalink()) }}"><span><img src="{{ asset('media/image/' ~ taxon.getPath()) }}" alt="" /></span></a>
                                <p class="banner-text">{{ taxon.getName() }}</p>
                            </div>
                        </div>
                    {% endfor %}
            </div>
        </div>
    </div>
    <!-- banner-area end -->
    <!--featured-product-area-start-->
    <section class="blog-area">
        <div class="container">
            <!--featured-product-area-heading-start-->
